{
  "name": "squibble_browser_client",
  "Name": "Squibble Browser Client",
  "version": "0.1.0",
  "pre_release": "pre-alpha 0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.3",
    "@tinymce/tinymce-react": "^4.1.0",
    "@uiw/react-md-editor": "^3.14.5",
    "bootstrap": "^5.1.3",
    "easymde": "^2.16.1",
    "md5": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-scripts": "5.0.1",
    "react-simplemde-editor": "^5.0.2",
    "rich-markdown-editor": "^11.21.3",
    "sass": "^1.53.0"
  },
  "scripts": {
    "bootstrap": "markserv bootstrap-5.2.0-beta1-examples",
    "start": "export REACT_APP_BUILD_ID=$(git rev-parse --short HEAD)  && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/node-logger": "^6.5.9",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "babel-plugin-named-exports-order": "^0.0.2",
    "dotenv-webpack": "^8.0.0",
    "prop-types": "^15.8.1",
    "rehype-sanitize": "^5.0.1",
    "styled-components": "^5.3.5",
    "webpack": "^5.73.0"
  },
  "resolutions": {
    "prosemirror-model": "1.16.1"
  }
}
